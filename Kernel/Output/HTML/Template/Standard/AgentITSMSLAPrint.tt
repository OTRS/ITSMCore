<table border="0" cellspacing="1" cellpadding="0" width="100%">
    <tr>
        <td class="mainhead">[% Translate("SLA") | html %]</td>
    </tr>
</table>
<table border="0" cellspacing="1" cellpadding="10" width="100%">
    <tr>
        <td class="contentkey">
            [% Translate("SLA-Info") | html %]:
            <table border="0" cellspacing="3" cellpadding="0" width="100%">
                <tr>
                    <td class="contentkey" width="20%">[% Translate("SLA") | html %]:</td>
                    <td class="contentvalue" width="30%">[% Data.Name | html %]</td>
                    <td class="contentkey" width="20%">[% Translate("Created") | html %]:</td>
                    <td class="contentvalue" width="30%">[% Data.CreateTime | Localize("TimeLong") %]</td>
                </tr>
                <tr>
                    <td class="contentkey"></td>
                    <td class="contentvalue"></td>
                    <td class="contentkey">[% Translate("Created by") | html %]:</td>
                    <td class="contentvalue">[% Data.CreateByName | html %]</td>
                </tr>
                <tr>
                    <td class="contentkey"></td>
                    <td class="contentvalue"></td>
                    <td class="contentkey">[% Translate("Last changed") | html %]:</td>
                    <td class="contentvalue">[% Data.ChangeTime | Localize("TimeLong") %]</td>
                </tr>
                <tr>
                    <td class="contentkey"></td>
                    <td class="contentvalue"></td>
                    <td class="contentkey">[% Translate("Last changed by") | html %]:</td>
                    <td class="contentvalue">[% Data.ChangeByName | html %]</td>
                </tr>
                <tr>
                    <td class="contentkey">&nbsp;</td>
                    <td colspan="3" class="contentvalue">&nbsp;</td>
                </tr>
            </table>
        </td>
    </tr>
</table>
<table border="0" width="640" cellpadding="0" cellspacing="0" bgcolor="#000000" class="container">
    <tr>
        <td>
            <table border="0" width="640" cellpadding="10" cellspacing="1" bgcolor="#ffffff">
                <tr>
                    <td>
                        <table border="0" width="100%" cellspacing="0" cellpadding="3">
                            <tr bgcolor="#ffffff">
                                <td valign="top" width="100%" class="small">
                                    <table border="0" width="100%" cellspacing="0" cellpadding="1">
                                        <tr>
                                            <td class="contentkey">[% Translate("SLA") | html %]:</td>
                                            <td class="contentvalue">[% Data.Name | html %]</td>
                                        </tr>
                                        <tr>
                                            <td class="contentkey">[% Translate("Type") | html %]:</td>
                                            <td class="contentvalue">[% Translate(Data.Type) | html %]</td>
                                        </tr>
                                        <tr>
                                            <td class="contentkey">[% Translate("Calendar") | html %]:</td>
                                            <td class="contentvalue">[% Translate(Data.CalendarName) | html %]</td>
                                        </tr>
                                        <tr>
                                            <td class="contentkey">[% Translate("First Response Time") | html %]:</td>
                                            <td class="contentvalue">[% Data.FirstResponseTime | html %] [% Translate("minutes") | html %]</td>
                                        </tr>
                                        <tr>
                                            <td class="contentkey">[% Translate("Update Time") | html %]:</td>
                                            <td class="contentvalue">[% Data.UpdateTime | html %] [% Translate("minutes") | html %]</td>
                                        </tr>
                                        <tr>
                                            <td class="contentkey">[% Translate("Solution Time") | html %]:</td>
                                            <td class="contentvalue">[% Data.SolutionTime | html %] [% Translate("minutes") | html %]</td>
                                        </tr>
                                        <tr>
                                            <td class="contentkey">[% Translate("Minimum Time Between Incidents") | html %]:</td>
                                            <td class="contentvalue">[% Data.MinTimeBetweenIncidents | html %] [% Translate("minutes") | html %]</td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>
